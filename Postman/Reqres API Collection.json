		{
			"name": "List Users",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Basic GET request should return status 200 and data\", function () {\r",
							"    pm.response.to.have.status(200); // Check if status is 200\r",
							"    pm.response.to.have.jsonBody();  // Check if response body is in JSON format\r",
							"    pm.response.to.not.have.body(null); // Ensure body is not null or empty\r",
							"});\r",
							"\r",
							"pm.test(\"GET request should return status 200\", function () {\r",
							"    pm.response.to.have.status(200); // Status code should be 200 OK\r",
							"});\r",
							"\r",
							"pm.test(\"Response time should be less than 2 seconds\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(2000); // Verify response time is below 2 seconds\r",
							"});\r",
							"\r",
							"pm.test(\"Response should have the correct data structure\", function () {\r",
							"    pm.response.to.have.jsonBody('data'); // Check if 'data' is present in the response\r",
							"    pm.response.to.have.jsonBody('data[0].id'); // Ensure id exists in the first item\r",
							"    pm.response.to.have.jsonBody('data[0].first_name'); // Ensure first name exists\r",
							"    pm.response.to.have.jsonBody('data[0].last_name'); // Ensure last name exists\r",
							"    pm.response.to.have.jsonBody('data[0].email'); // Ensure email exists\r",
							"});\r",
							"\r",
							"pm.test(\"Pagination should work correctly\", function () {\r",
							"    pm.response.to.have.jsonBody('page'); // Ensure pagination is present\r",
							"    pm.response.to.have.jsonBody('per_page'); // Check if per_page is in the response\r",
							"    pm.response.to.have.jsonBody('total'); // Ensure total number of users is available\r",
							"});\r",
							"\r",
							"pm.test(\"Request should return empty data if no results\", function () {\r",
							"    let data = pm.response.json().data;\r",
							"    pm.expect(data.length).to.eql(6); // Ensure that data length is 0 (empty data)\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Data should be sorted in ascending order\", function () {\r",
							"    let users = pm.response.json().data;\r",
							"    let isSorted = true;\r",
							"\r",
							"    // Check if users are sorted by id\r",
							"    for (let i = 0; i < users.length - 1; i++) {\r",
							"        if (users[i].id > users[i + 1].id) {\r",
							"            isSorted = false;\r",
							"            break;\r",
							"        }\r",
							"    }\r",
							"    pm.expect(isSorted).to.eql(true); // Ensure that users are sorted by ID\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"id": "31f28673-e4d8-4884-9860-00218d52dde1"
					}
				}
